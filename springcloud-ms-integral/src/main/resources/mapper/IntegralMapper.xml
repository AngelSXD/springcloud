<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.swapping.springcloud.ms.integral.mapper.IntegralMapper">

    <insert id="insertIntegral" parameterType="com.swapping.springcloud.ms.integral.domain.Integral">
        INSERT INTO
        integral
        (
        id,create_date,update_date,update_id,create_id,uid,member_uid,integral_num
        )
        VALUES
        (
        #{id},#{createDate},#{updateDate},#{updateId},#{createId},#{uid},#{memberUid},#{integralNum}
        )
    </insert>

</mapper>

